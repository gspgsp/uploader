{
    "name": "gsp/uploader",
    "description": "a package to improve upload experience",
    "type": "library",
    "license":"MIT",
    "autoload": {
        "psr-4": {
            "Gsp\\Uploader\\": "src/"
        }
    },
    "authors": [
        {
            "name": "gsp",
            "email": "guoshipengdss@gmail.com"
        }
    ],
    "minimum-stability":"dev",
